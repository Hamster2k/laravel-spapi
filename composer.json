{
    "name": "highsidelabs/laravel-spapi",
    "type": "library",
    "description": "A Laravel wrapper for Amazon's Selling Partner API (via jlevers/selling-partner-api)",
    "license": "BSD-3-Clause",
    "keywords": [
        "laravel",
        "wrapper",
        "selling-partner-api",
        "sp-api",
        "amazon",
        "ecommerce",
        "sdk",
        "rest"
    ],
    "authors": [
        {
            "name": "Jesse Evers",
            "email": "jesse@highsidelabs.co"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^9.41|^10.0|^11.0",
        "illuminate/database": "^9.41|^10.0|^11.0",
        "illuminate/cache": "^9.41|^10.0|^11.0",
        "jlevers/selling-partner-api": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "phpunit/phpunit": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "HighsideLabs\\LaravelSpApi\\": "src/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix ."
    },
    "extra": {
        "laravel": {
            "providers": [
                "HighsideLabs\\LaravelSpApi\\SellingPartnerApiServiceProvider"
            ]
        }
    }
}
